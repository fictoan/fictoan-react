{
    "name": "fictoan-react",
    "version": "0.32.14",
    "private": false,
    "main": "dist/cjs/index.js",
    "module": "dist/es/index.js",
    "jsnext:main": "dist/es/index.js",
    "sideEffects": false,
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fictoan/fictoan-react"
    },
    "license": "MIT",
    "scripts": {
        "start": "rollup -c -w",
        "lint": "eslint '**/*.tsx' --ignore-pattern node_modules/",
        "prebuild": "rm -rf dist",
        "build": "rollup -c",
        "predeploy": "$npm_execpath run build",
        "deploy": "$npm_execpath publish"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/preset-react": "^7.14.5",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-url": "^4.0.2",
        "@svgr/rollup": "^5.5.0",
        "@types/lodash-es": "^4.17.4",
        "@types/react": "^17.0.11",
        "@types/react-syntax-highlighter": "^13.5.1",
        "@types/styled-components": "^5.1.10",
        "@typescript-eslint/eslint-plugin": "^4.28.0",
        "@typescript-eslint/parser": "^4.28.0",
        "agadoo": "^2.0.0",
        "eslint": "^7.29.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-tree-shaking": "^1.9.2",
        "react": "^17.0.2",
        "rollup": "^2.52.3",
        "rollup-plugin-rename-node-modules": "^1.1.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.26.0",
        "rollup-plugin-visualizer": "5.5.0",
        "styled-components": "^5.3.0",
        "tslib": "^1.11.1",
        "typescript": "^3.8.2",
        "typescript-plugin-styled-components": "^1.5.0"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "lodash-es": "^4.17.21",
        "polished": "^4.1.3",
        "react-syntax-highlighter": "^15.4.3"
    },
    "peerDependencies": {
        "react": "16.x || 17.x",
        "styled-components": "^5.1.1"
    },
    "optionalDependencies": {
        "@emotion/is-prop-valid": "^1.1.0"
    }
}
