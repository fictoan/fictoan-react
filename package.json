{
    "name": "fictoan-react",
    "version": "0.34.0",
    "private": false,
    "main": "dist/cjs/index.js",
    "module": "dist/es/index.js",
    "jsnext:main": "dist/es/index.js",
    "sideEffects": [
        "**/*.css"
    ],
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fictoan/fictoan-react"
    },
    "license": "MIT",
    "scripts": {
        "start": "rollup -c -w",
        "prebuild": "rm -rf dist",
        "build": "rollup -c",
        "predeploy": "$npm_execpath run build",
        "deploy": "$npm_execpath publish",
        "format": "prettier --write $PWD/'src/**/*.[jt]s?(x)'"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.[jt]s?(x)": "prettier --write"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.14.5",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-url": "^6.0.0",
        "@svgr/rollup": "^5.5.0",
        "@types/lodash-es": "^4.17.4",
        "@types/react": "^17.0.13",
        "@types/react-syntax-highlighter": "^13.5.1",
        "@types/styled-components": "^5.1.11",
        "autoprefixer": "^10.3.1",
        "husky": "^7.0.1",
        "lint-staged": "^11.1.1",
        "prettier": "^2.3.2",
        "react": "^17.0.2",
        "rollup": "^2.52.7",
        "rollup-plugin-progress": "^1.1.2",
        "rollup-plugin-styles": "^3.14.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "rollup-plugin-visualizer": "5.5.1",
        "styled-components": "^5.3.0",
        "tslib": "^2.3.0",
        "typescript": "^4.3.5",
        "typescript-plugin-styled-components": "^1.6.0"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "lodash-es": "^4.17.21",
        "polished": "^4.1.3",
        "prism-react-renderer": "^1.2.1"
    },
    "peerDependencies": {
        "react": "16.x || 17.x",
        "styled-components": "^5.1.1"
    },
    "optionalDependencies": {
        "@emotion/is-prop-valid": "^1.1.0"
    }
}
